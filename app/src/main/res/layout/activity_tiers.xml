<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_tiers"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">

    <TextView
        android:text="S3 Overwatch Tier List"
        android:textSize="30dp"
        android:textAppearance="@style/Base.ThemeOverlay.AppCompat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView5" />

    <TextView
        android:text="S"
        android:textSize="50dp"
        android:textAppearance="@style/Base.ThemeOverlay.AppCompat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:layout_marginTop="13dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Lucio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Lucio"
        android:layout_alignBaseline="@+id/reinhardt"
        android:layout_alignBottom="@+id/reinhardt"
        android:layout_toEndOf="@+id/reinhardt" />

    <Button
        android:text="Zarya"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:id="@+id/zarya"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignStart="@+id/Ana" />

    <Button
        android:text="D.Va"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/mercy"
        android:layout_toEndOf="@+id/mercy"
        android:id="@+id/dva" />

    <TextView
        android:text="A"
        android:textSize="50dp"
        android:textAppearance="@style/Base.ThemeOverlay.AppCompat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/reinhardt"
        android:layout_alignStart="@+id/reinhardt" />

    <Button
        android:text="Ana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Ana"
        android:layout_below="@+id/reinhardt"
        android:layout_toEndOf="@+id/textView3" />

    <TextView
        android:text="B"
        android:textSize="50dp"
        android:textAppearance="@style/Base.ThemeOverlay.AppCompat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_below="@+id/Ana"
        android:layout_toStartOf="@+id/zarya" />

    <Button
        android:text="Mercy"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:id="@+id/mercy"
        android:layout_alignBaseline="@+id/zarya"
        android:layout_alignBottom="@+id/zarya"
        android:layout_toEndOf="@+id/zarya" />

    <Button
        android:text="Bastion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/Dtier"
        android:id="@+id/bastion" />

    <Button
        android:text="Torbjorn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/zarya"
        android:id="@+id/torbjorn" />

    <TextView
        android:text="F"
        android:textSize="50dp"
        android:textAppearance="@style/Base.ThemeOverlay.AppCompat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Ftier"
        android:layout_alignTop="@+id/bastion"
        android:layout_alignEnd="@+id/textView2" />

    <Button
        android:text="Widowmaker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/widowmaker"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/torbjorn" />

    <Button
        android:text="Hanzo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/hanzo"
        android:layout_above="@+id/bastion"
        android:layout_toEndOf="@+id/Dtier" />

    <TextView
        android:text="D"
        android:textSize="50dp"
        android:textAppearance="@style/Base.ThemeOverlay.AppCompat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Dtier"
        android:layout_alignTop="@+id/hanzo"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Sombra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/hanzo"
        android:layout_alignStart="@+id/torbjorn"
        android:id="@+id/sombra" />

    <Button
        android:text="Tracer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/torbjorn"
        android:layout_toEndOf="@+id/torbjorn"
        android:id="@+id/tracer" />

    <Button
        android:text="Zenyatta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/zenyatta"
        android:layout_alignBaseline="@+id/dva"
        android:layout_alignBottom="@+id/dva"
        android:layout_toEndOf="@+id/dva" />

    <Button
        android:text="Reinhardt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/reinhardt"
        android:layout_alignTop="@+id/textView3"
        android:layout_toEndOf="@+id/textView3" />

    <Button
        android:text="Roadhog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/roadhog"
        android:layout_below="@+id/zarya"
        android:layout_toEndOf="@+id/textView2" />

    <Button
        android:text="Symmetra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/roadhog"
        android:layout_toEndOf="@+id/hanzo"
        android:id="@+id/symmetra" />

    <Button
        android:text="Soldier: 76"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/soldier76"
        android:layout_below="@+id/dva"
        android:layout_toEndOf="@+id/symmetra" />

    <TextView
        android:text="C"
        android:textSize="50dp"
        android:textAppearance="@style/Base.ThemeOverlay.AppCompat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:layout_below="@+id/roadhog"
        android:layout_toStartOf="@+id/button18" />

    <Button
        android:text="Genji"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/genji"
        android:layout_below="@+id/roadhog"
        android:layout_alignStart="@+id/roadhog" />

    <Button
        android:text="McCree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/roadhog"
        android:layout_toStartOf="@+id/tracer"
        android:id="@+id/mccree" />

    <Button
        android:text="Pharah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/genji"
        android:layout_toStartOf="@+id/mccree"
        android:id="@+id/Pharah" />

    <Button
        android:text="Reaper"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/hanzo"
        android:layout_alignEnd="@+id/Pharah"
        android:id="@+id/reaper" />

    <Button
        android:text="Winston"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/symmetra"
        android:layout_toEndOf="@+id/mccree"
        android:id="@+id/winston" />

    <Button
        android:text="Junkrat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/reaper"
        android:layout_alignStart="@+id/symmetra"
        android:id="@+id/junkrat" />

    <Button
        android:text="Mei"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/Pharah"
        android:layout_toEndOf="@+id/mccree"
        android:id="@+id/mei" />

</RelativeLayout>
